<Window x:Class="KorisnickiInterfejs.IzvestajKapije"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:rv="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        xmlns:local="clr-namespace:KorisnickiInterfejs"
        Loaded="Window_Loaded"
        mc:Ignorable="d"
        Title="IzvestajKapije" Height="450" Width="800">
    <Border Padding="15">
        <StackPanel>
            <WindowsFormsHost Width="500" Height="400">
                <rv:ReportViewer x:Name="rvKapije"/>
            </WindowsFormsHost>
        </StackPanel>
    </Border>
</Window>
